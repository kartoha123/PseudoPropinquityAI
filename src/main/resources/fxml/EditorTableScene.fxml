<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="382.0" prefWidth="490.0" 
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="com.bunjlabs.pseudopropinquityai.editor.FXMLController">
   <children>
      <TableView fx:id="table" AnchorPane.bottomAnchor="40.0" 
                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" 
                 AnchorPane.topAnchor="0.0" >
        <columns>
          <TableColumn fx:id="columnId" prefWidth="59.0" text="id" />
          <TableColumn fx:id="columnFA" prefWidth="147.0" text="figure A" />
            <TableColumn fx:id="columnFB" prefWidth="157.0" text="figure B" />
            <TableColumn fx:id="columnProp" prefWidth="102.0" text="propinquity" />
        </columns>
      </TableView>
      <Button fx:id="addBtn" onAction="#handleAddButtonAction" 
              layoutX="14.0" layoutY="356.0" mnemonicParsing="false" text="Add new" 
              AnchorPane.bottomAnchor="7.0" AnchorPane.leftAnchor="7.0" />
      <Button fx:id="editBtn" onAction="#handleEditButtonAction" 
              layoutX="408.0" layoutY="350.0" mnemonicParsing="false" 
              text="Edit" AnchorPane.bottomAnchor="7.0" AnchorPane.rightAnchor="7.0" />
   </children>
</AnchorPane>
